{
  "manifest_version": 2,
  "version": "0.0.1",
  "name": "ModularGrid Random",
  "description": "Adds random button to Eurorack browsing in ModularGrid",
  "permissions": [ "activeTab" ],
  "content_scripts": [
    {
      "matches": [ "*://*.modulargrid.net/e/*" ],
      "css": ["style/style.css"],
      "js": [
        "scripts/requests.js",
        "scripts/scrape.js",
        "scripts/viewHelpers.js",
        "scripts/embedded.js"
      ]
    }
  ]
}
